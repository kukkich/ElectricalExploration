{
  "App": {
    "FunctionalOptimizer": {
      "Betta": 1,
      "MaxIteration": 6,
      "Precision": 1e-8
    },
    "GaussZeidel": {
      "MaxIteration": 1000,
      "Precision": 1e-8,
      "Relaxation": 1
    },
    "LOS": {
      "Eps": 1e-8,
      "MaxIterations": 1000
    }
  },
  "Serilog": {
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:HH:mm:ss:fff} [{SourceContext}]: {Message:lj}{NewLine}{Exception}"
          //"outputTemplate": "{Timestamp:HH:mm:ss:fff} [{SourceContext}]: {NewLine}  {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "../../../Logs/log.txt",
          "outputTemplate": "{Timestamp:HH:mm:ss:fff} [{SourceContext}]: {Message:lj}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithThreadId" ],
    "Properties": {
      "Application": "InverseTask"
    }
  }
}