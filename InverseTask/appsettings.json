{
  "App": {
    "FunctionalOptimizer": {
      "Betta": 1,
      "MaxIteration": 100,
      "Precision": 1e-10
    },
    "GaussZeidel": {
      "MaxIteration": 1000,
      "Precision": 1e-8,
      "Relaxation": 1
    },
    "LOS": {
      "Eps": 1e-8,
      "MaxIterations": 1000
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "InverseTask.EquationSystem": "Error",
        "InverseTask.FunctionalOptimizer": "Debug"
        //"InverseTask.FunctionalOptimizer": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Message:lj}{NewLine}{Exception}"
          //"outputTemplate": "{Level} {Timestamp:HH:mm:ss:fff} [{SourceContext}]: {NewLine}  {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "../../../Logs/log.txt",
          "outputTemplate": "{Message:lj}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithThreadId" ],
    "Properties": {
      "Application": "InverseTask"
    }
  }
}